def maxOfTwo(a, b): #Славо богу переменых только 2
    if a > b: # Если а больше б 
        print(a)
    elif a < b: 
        print(b) # тут наоборот
    else :
        print ("Неожиданый ввод чисил и они разумеються равны между собой") # Если вдруг введете одинаковые цифры
a = float(input("Введіть перше число: ")) # float вместо int вдруг  число с сотыми 
b = float(input("Введіть друге число: ")) #
maxOfTwo(a, b)
